  "id": "<%= config[:category_retrieve_id] %>",
  "name": "<%= config[:category_retrieve_name] %>",
  "description": "<%= config[:category_retrieve_description] %>",
  "slug": "playlist-sample",
  "type": "playlist",
  "orderNumber": <%= config[:category_retrieve_order_number] %>,
  "icon": "https:///example.com/image002.png",
  "status": 1,
