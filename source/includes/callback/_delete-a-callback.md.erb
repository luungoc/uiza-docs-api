```go
import (
  uiza "github.com/uizaio/api-wrapper-go"
  "github.com/uizaio/api-wrapper-go/callback"
)

func init() {
  Uiza.Authorization = "<%= config[:token] %>"
}

params := &uiza.CallbackIDParams{ID: uiza.String("<%= config[:callback_delete_id] %>")}
response, err := callback.Delete(params)
if err != nil {
  log.Printf("%v\n", err)
} else {
  log.Printf("%v\n", response)
}
